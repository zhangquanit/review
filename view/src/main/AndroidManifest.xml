<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar"
        android:supportsRtl="true">
        <activity android:name="com.review.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.review.view.custom.TestActivity"  />
        <activity android:name="com.review.view.custom.CustomActivity" />
        <activity android:name="com.review.view.views.FlowLayoutActivity" />
    </application>

</manifest>